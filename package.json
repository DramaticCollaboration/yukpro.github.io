{
  "name": "vue-quasar-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:https": "vite --host --https",
    "build": "vue-tsc --noEmit && vite build",
    "build:pwa": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview --https",
    "preview:pwa": "vite preview --https --port 4173",
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html --chrome-flags=\"--ignore-certificate-errors\""
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "pinia": "^3.0.3",
    "quasar": "^2.18.1",
    "vue": "^3.5.17",
    "vue-router": "4.5.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.9.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "sass": "1.89.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-pwa": "^1.0.1",
    "vue-tsc": "^2.1.6",
    "@vitejs/plugin-basic-ssl": "^2.1.0"
  }
}
